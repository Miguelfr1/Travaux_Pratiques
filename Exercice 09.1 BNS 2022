def calcul(k: int) :
    m = []
    m.append(k)
    while k > 1 :
        if (k % 2) == 0 :
            k = k // 2
            m.append(k)
        else :
            k = k * 3 + 1
            m.append(k)
    return m
    
    
    
    
    
 calcul(4) == [7, 22, 11, 34, 17, 52, 26, 13, 40, 20, 10, 5, 16, 8, 4, 2, 1]
